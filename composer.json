{
    "name": "andrewdyer/predis-adaptor",
    "description": "A simple framework-agnostic caching solution using Redis",
    "type": "library",
    "keywords": ["php", "redis", "predis", "adaptor", "framework-agnostic"],
    "homepage": "https://github.com/andrewdyer/predis-adaptor",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Dyer",
            "homepage": "https://andrewdyer.rocks",
            "role": "developer"
        }
    ],
    "support": {
        "source": "https://github.com/andrewdyer/predis-adaptor.git"
    },
    "require": {
        "php": ">=8.2",
        "predis/predis": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Anddye\\PredisAdaptor\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
